    call CampaignAttackerEx( 1, 2, 3, GALACTIC_REPUBLIC_JEDI )
    call CampaignAttackerEx( 2, 2, 3, GALACTIC_REPUBLIC_ATTE )
    call AddStandardUnitsToAttackWave()
    call Sleep( M2 ) // Waits 2 minutes before attacking
    call LaunchAttack()

    //*** WAVE 7 ***
