function AttackWaves takes nothing returns nothing
    //*** WAVE 1 ***
    call InitAssaultGroup()
    call CampaignAttackerEx( 2, 3, 3, GALACTIC_REPUBLIC_CLONE_TROOPER )
    call CampaignAttackerEx( 0, 1, 2, GALACTIC_REPUBLIC_ARC_TROOPER )
    call AddStandardUnitsToAttackWave()
    call Sleep( M3 ) // Waits 3 minutes before attacking
    call LaunchAttack()

    //*** WAVE 1 ***
    call InitAssaultGroup()
    call CampaignAttackerEx( 2, 3, 3, GALACTIC_REPUBLIC_CLONE_TROOPER )
    call CampaignAttackerEx( 0, 1, 2, GALACTIC_REPUBLIC_ARC_TROOPER )
    call AddStandardUnitsToAttackWave()
    call Sleep( M2 ) // Waits 2 minutes before attacking
    call LaunchAttack()

    //*** WAVE 2 ***
    call InitAssaultGroup()
    call CampaignAttackerEx( 3, 4, 4, GALACTIC_REPUBLIC_CLONE_TROOPER )
    call CampaignAttackerEx( 1, 2, 3, GALACTIC_REPUBLIC_ARC_TROOPER )
    call AddStandardUnitsToAttackWave()
    call Sleep( M1 ) // Waits 1 minute before attacking
    call LaunchAttack()

    //*** WAVE 3 ***
    call InitAssaultGroup()
    call CampaignAttackerEx( 3, 4, 4, GALACTIC_REPUBLIC_CLONE_TROOPER )
    call CampaignAttackerEx( 1, 2, 3, GALACTIC_REPUBLIC_ARC_TROOPER )
    call CampaignAttackerEx( 1, 2, 3, GALACTIC_REPUBLIC_ATRT )
    call AddStandardUnitsToAttackWave()
    call Sleep( M2 ) // Waits 2 minutes before attacking
    call LaunchAttack()


    //*** WAVE 4 ***
