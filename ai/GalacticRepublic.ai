    call CampaignAttackerEx( 2, 2, 3, GALACTIC_REPUBLIC_ATTE )
    call CampaignAttackerEx( 2, 2, 3, GALACTIC_REPUBLIC_ATAP )
    call CampaignAttackerEx( 2, 2, 3, GALACTIC_REPUBLIC_CLONE_TURBO_TANK )
