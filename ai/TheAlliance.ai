		// big air army
		call CampaignAttackerEx( 7, 10, 12, NAGA_COUATL )

		call CampaignAttackerEx( 7, 10, 12, HUMAN_DRAGON_HAWK )

		call AddStandardUnitsToAttackWave()
		call Sleep( M2 ) // Waits 2 minutes before attacking
		call LaunchAttack()
    endloop
endfunction

