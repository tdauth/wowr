    if ( not ( TimerGetRemaining(udg_VotekickCooldownPlayerTimer[GetConvertedPlayerId(GetTriggerPlayer())]) <= 0.00 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Chat_Command_Votekick_Actions takes nothing returns nothing
    if ( Trig_Chat_Command_Votekick_Func001C() ) then
        if ( Trig_Chat_Command_Votekick_Func001Func001C() ) then
            set udg_TmpInteger=S2I(SubStringBJ(GetEventPlayerChatString(), StringLength("-votekick "), StringLength(GetEventPlayerChatString())))
            set udg_TmpPlayer=ConvertedPlayer(udg_TmpInteger)
            set udg_TmpString=GetPlayerNameColored(udg_TmpPlayer)
            if ( Trig_Chat_Command_Votekick_Func001Func001Func004C() ) then
                if ( Trig_Chat_Command_Votekick_Func001Func001Func004Func001C() ) then
                    set udg_TmpString2=GetPlayerNameColored(GetTriggerPlayer())
                    set udg_VotekickTargetPlayer=udg_TmpPlayer
                    set udg_VotekickVotesYesCounter=( udg_VotekickVotesYesCounter + 1 )
                    set udg_VotekickPlayerHasVoted[GetConvertedPlayerId(GetTriggerPlayer())]=true
                    call StartTimerBJ(udg_VotekickRunningTimer, false, 30.00)
                    call CreateTimerDialogBJ(GetLastCreatedTimerBJ(), ( "Votekick against " + udg_TmpString ))
                    set udg_VotekickRunningTimerWindow=GetLastCreatedTimerDialogBJ()
                    set udg_VotekickIsRunning=true
                    call DisplayTextToForce(GetPlayersAll(), ( udg_TmpString2 + ( " has started a votekick against " + ( udg_TmpString + "! Enter \"-yes\" to vote for kicking the player from the game." ) ) ))
                    call ConditionalTriggerExecute(gg_trg_Votekick_Calculate)
                else
                    call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), ( "You cannot start a votekick against player " + ( udg_TmpString + "!" ) ))
                endif
            else
                call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), "TRIGSTR_6225")
            endif
        else
            call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), ( "Wait until you can start a votekick again: " + ( R2SW(TimerGetRemaining(udg_VotekickCooldownTimer), 1, 2) + " seconds." ) ))
        endif
    else
        call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), ( "Wait until you can start a votekick again: " + ( R2SW(TimerGetRemaining(udg_VotekickCooldownPlayerTimer[GetConvertedPlayerId(GetTriggerPlayer())]), 1, 2) + " seconds." ) ))
    endif
