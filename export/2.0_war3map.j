//===========================================================================
// Trigger: Game Start
//===========================================================================
function Trig_Game_Start_Func006Func001Func001C takes nothing returns boolean
    if ( ( GetPlayerSlotState(GetOwningPlayer(GetEnumUnit())) == PLAYER_SLOT_STATE_EMPTY ) ) then
        return true
    endif
    if ( ( GetPlayerController(GetOwningPlayer(GetEnumUnit())) == MAP_CONTROL_COMPUTER ) ) then
        return true
    endif
    return false
endfunction

function Trig_Game_Start_Func006Func001C takes nothing returns boolean
    if ( not Trig_Game_Start_Func006Func001Func001C() ) then
        return false
    endif
    return true
endfunction

function Trig_Game_Start_Func006A takes nothing returns nothing
    if ( Trig_Game_Start_Func006Func001C() ) then
        call RemoveUnit(GetEnumUnit())
    else
        call SelectUnitForPlayerSingle(GetEnumUnit(), GetOwningPlayer(GetEnumUnit()))
    endif
endfunction

function Trig_Game_Start_Func007Func002A takes nothing returns nothing
    call SetPlayerAllianceBJ(udg_TmpPlayer, ALLIANCE_SHARED_VISION, false, GetEnumPlayer())
    call SetPlayerAllianceBJ(GetEnumPlayer(), ALLIANCE_SHARED_VISION, false, udg_TmpPlayer)
endfunction

function Trig_Game_Start_Func007A takes nothing returns nothing
    set udg_TmpPlayer=GetEnumPlayer()
    call ForForce(GetPlayersAllies(udg_TmpPlayer), function Trig_Game_Start_Func007Func002A)
    call CreateFogModifierRectBJ(true, GetEnumPlayer(), FOG_OF_WAR_VISIBLE, gg_rct_Hero_Selection_Visibility)
    set udg_VisibleModifierVisible[GetConvertedPlayerId(GetEnumPlayer())]=GetLastCreatedFogModifier()
    call CreateFogModifierRectBJ(false, GetEnumPlayer(), FOG_OF_WAR_MASKED, gg_rct_Hero_Selection_Visibility)
    set udg_VisibleModifierBlackMask[GetConvertedPlayerId(GetEnumPlayer())]=GetLastCreatedFogModifier()
    call CreateFogModifierRectBJ(true, GetEnumPlayer(), FOG_OF_WAR_MASKED, gg_rct_Secret_Hero_Chamber)
    set udg_VisibleModifierBlackMaskSecret[GetConvertedPlayerId(GetEnumPlayer())]=GetLastCreatedFogModifier()
    call CreateFogModifierRectBJ(false, GetEnumPlayer(), FOG_OF_WAR_VISIBLE, gg_rct_Secret_Hero_Chamber)
    set udg_VisibleModifierVisibleSecret[GetConvertedPlayerId(GetEnumPlayer())]=GetLastCreatedFogModifier()
endfunction

function Trig_Game_Start_Func010A takes nothing returns nothing
    set udg_TmpPlayer=GetEnumPlayer()
    call ConditionalTriggerExecute(gg_trg_Chat_Command_Stats_Update_Tier_Level)
    set udg_TmpPlayer=GetEnumPlayer()
    call ConditionalTriggerExecute(gg_trg_Chat_Command_Stats_Update_Workers)
endfunction

function Trig_Game_Start_Actions takes nothing returns nothing
    // Disable portals until players have voted
    call ConditionalTriggerExecute(gg_trg_Disable_Location_Portals)
    // Boss markers
    call ConditionalTriggerExecute(gg_trg_Boss_Markers)
    // Hero Selection
    call ForGroupBJ(GetUnitsInRectAll(gg_rct_Hero_Selection), function Trig_Game_Start_Func006A)
    call ForForce(GetPlayersAll(), function Trig_Game_Start_Func007A)
    call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_1221")
    // Player Stats
    call ForForce(GetPlayersAll(), function Trig_Game_Start_Func010A)
    // Game Settings
    call ConditionalTriggerExecute(gg_trg_Game_Settings_Dialog)
    // Floating Texts
    call ConditionalTriggerExecute(gg_trg_Floating_Texts)
    // Clan Deposit Cooldown
    call StartTimerBJ(udg_ClanDepositCooldownTimer, false, 1800.00)
endfunction

