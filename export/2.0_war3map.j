    if ( not ( udg_TmpBoolean == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Evolution_Available_and_for_Creeps_Actions takes nothing returns nothing
    // Enable upgrade Evolution
    if ( Trig_Evolution_Available_and_for_Creeps_Func006C() ) then
        call SetPlayerTechResearchedSwap('R00X', 1, GetOwningPlayer(GetTriggerUnit()))
    else
        call DoNothing()
    endif
    // Determine if it is the lowest hero
    set udg_TmpBoolean=false
    call GroupClear(udg_TmpGroup)
    call ForForce(GetPlayersMatching(Condition(function Trig_Evolution_Available_and_for_Creeps_Func010001001)), function Trig_Evolution_Available_and_for_Creeps_Func010A)
    set udg_TmpUnit=GroupPickRandomUnit(udg_TmpGroup)
    call ForGroupBJ(udg_TmpGroup, function Trig_Evolution_Available_and_for_Creeps_Func012A)
    if ( Trig_Evolution_Available_and_for_Creeps_Func013C() ) then
        set udg_TmpBoolean=true
        // When you are in repick your hero might be no unit. We need to consider the lowest XP as well.
        call ForForce(GetPlayersMatching(Condition(function Trig_Evolution_Available_and_for_Creeps_Func013Func003001001)), function Trig_Evolution_Available_and_for_Creeps_Func013Func003A)
    else
        call DoNothing()
    endif
    // Creeps
    if ( Trig_Evolution_Available_and_for_Creeps_Func015C() ) then
        set udg_TmpInteger=GetHeroLevel(GetTriggerUnit())
        set udg_TmpInteger=( udg_TmpInteger / 10 )
        set udg_TmpInteger=( udg_TmpInteger * 10 )
        if ( Trig_Evolution_Available_and_for_Creeps_Func015Func005C() ) then
            call SetPlayerTechResearchedSwap('R00X', 1, Player(PLAYER_NEUTRAL_AGGRESSIVE))
            call SetPlayerTechResearchedSwap('R00X', 1, udg_BossesPlayer)
            set udg_EvolutionLevel=udg_TmpInteger
            // Set upgrade levels. Upgrades are split due to the limit of 100 levels in Warcraft.
            call SetEvolutionLevelOfPlayer(Player(PLAYER_NEUTRAL_AGGRESSIVE) , udg_EvolutionLevel)
            call SetEvolutionLevelOfPlayer(udg_BossesPlayer , udg_EvolutionLevel)
            set udg_TmpString=GetPlayerNameColored(GetOwningPlayer(GetTriggerUnit()))
            call QuestMessageBJ(GetPlayersAll(), bj_QUESTMESSAGE_ALWAYSHINT, ( "Evolution of all creeps on the map is upgraded to level " + ( I2S(udg_EvolutionLevel) + ( " from a hero of " + ( udg_TmpString + "!" ) ) ) ))
            set udg_TmpPlayer=Player(PLAYER_NEUTRAL_AGGRESSIVE)
            call ConditionalTriggerExecute(gg_trg_Evolution_Upgrade)
            set udg_TmpPlayer=udg_BossesPlayer
            call ConditionalTriggerExecute(gg_trg_Evolution_Upgrade)
        else
            call DoNothing()
        endif
    else
        call DoNothing()
    endif
endfunction

