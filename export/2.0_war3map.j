                call SetPlayerTechResearchedSwap('R028', 0, udg_TmpPlayer)
            endif
        else
            call SetPlayerMaxHeroesAllowed(1, udg_TmpPlayer)
            call SetPlayerTechResearchedSwap('R028', 0, udg_TmpPlayer)
            if ( Trig_Outland_Taverns_Update_Hero_Counter_Func001Func001Func003C() ) then
                call SetPlayerStateBJ(udg_TmpPlayer, PLAYER_STATE_RESOURCE_HERO_TOKENS, 1)
            else
                call SetPlayerStateBJ(udg_TmpPlayer, PLAYER_STATE_RESOURCE_HERO_TOKENS, 0)
            endif
        endif
    endif
