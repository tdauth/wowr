//===========================================================================
// Trigger: Become Demigod Light
//
// udg_TmpUnit - the hero
//===========================================================================
function Trig_Become_Demigod_Light_Actions takes nothing returns nothing
    local unit tmpUnit= udg_TmpUnit
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=6
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call UnitRemoveItemSwapped(UnitItemInSlotBJ(udg_TmpUnit, GetForLoopIndexA()), udg_TmpUnit)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    set udg_DemigodXP[GetConvertedPlayerId(GetOwningPlayer(udg_TmpUnit))]=GetHeroXP(udg_Held[GetConvertedPlayerId(GetOwningPlayer(udg_TmpUnit))])
    call CreateNUnitsAtLoc(1, 'H003', GetOwningPlayer(udg_TmpUnit), GetRectCenter(gg_rct_Temple_Center), 270.00)
    set udg_Held[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=GetLastCreatedUnit()
    set udg_Hero[( GetConvertedPlayerId(GetOwningPlayer(udg_TmpUnit)) - 1 )]=GetLastCreatedUnit()
    // Triggers events which change udg_TmpUnit
    call SetHeroXP(GetLastCreatedUnit(), udg_DemigodXP[GetConvertedPlayerId(GetOwningPlayer(udg_TmpUnit))], true)
    set udg_TmpUnit=tmpUnit
    call RefreshRucksackForPlayer(GetPlayerId(GetOwningPlayer(udg_TmpUnit)))
    set udg_TmpString=GetPlayerNameColored(GetOwningPlayer(udg_TmpUnit))
    call DisplayTimedTextToForce(GetPlayersAll(), 30, ( udg_TmpString + " has become an \"Eternal Guardian\" (demigod)!" ))
    call RemoveUnit(udg_TmpUnit)
endfunction

