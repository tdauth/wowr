function Trig_Player_Leaves_Actions takes nothing returns nothing
    set udg_TmpString=GetPlayerNameColored(GetTriggerPlayer())
    call DisplayTextToForce(GetPlayersAll(), ( udg_TmpString + " has left the game." ))
    call SetForceAllianceStateBJ(GetForceOfPlayer(GetTriggerPlayer()), bj_FORCE_PLAYER[PLAYER_NEUTRAL_AGGRESSIVE], bj_ALLIANCE_UNALLIED)
    call SetForceAllianceStateBJ(GetForceOfPlayer(GetTriggerPlayer()), GetForceOfPlayer(udg_BossesPlayer), bj_ALLIANCE_UNALLIED)
    call SetForceAllianceStateBJ(GetForceOfPlayer(GetTriggerPlayer()), GetForceOfPlayer(udg_TheBurningLegion), bj_ALLIANCE_UNALLIED)
    call ConditionalTriggerExecute(gg_trg_Player_Leaves_Recalculate_All_Votes)
    if ( Trig_Player_Leaves_Func007C() ) then
        call SetUnitPositionLoc(udg_Held[GetConvertedPlayerId(GetTriggerPlayer())], GetRectCenter(gg_rct_redirect_forbidden_zone))
        call ReviveHeroLoc(udg_Held[GetConvertedPlayerId(GetTriggerPlayer())], GetRectCenter(gg_rct_redirect_forbidden_zone), true)
        call DropAllItemsFromHero(udg_Hero[GetPlayerId(GetTriggerPlayer())])
    else
        call DoNothing()
    endif
    if ( Trig_Player_Leaves_Func008C() ) then
        call DropBackpackForPlayer(GetPlayerId(GetTriggerPlayer()) , gg_rct_redirect_forbidden_zone)
        // Destroy the backpack since we cannot use it in full shared control properly
        call DestroyRucksackSystemForPlayer(GetPlayerId(GetTriggerPlayer()))
    else
        call DoNothing()
    endif
endfunction

