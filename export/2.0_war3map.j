        return false
    endif
    return true
endfunction

function Trig_Dwarf_Lumber_Mill_Auto_Return_Func003A takes nothing returns nothing
    local location tmpLocation= null
    set udg_TmpHandle=GetEnumUnit()
    set udg_TmpDestructible=LoadDestructableHandleBJ(0, GetHandleIdBJ(udg_TmpHandle), udg_DwarfLumberMillTrees)
    if ( Trig_Dwarf_Lumber_Mill_Auto_Return_Func003Func004C() ) then
        set tmpLocation=GetUnitLoc(GetEnumUnit())
        set udg_TmpDestructible=RandomLivingTreeDestructableInCircle(4000.0 , tmpLocation)
        call RemoveLocation(tmpLocation)
        set tmpLocation=null
        if ( Trig_Dwarf_Lumber_Mill_Auto_Return_Func003Func004Func007C() ) then
            call IssueTargetDestructableOrder(GetEnumUnit(), "harvest", udg_TmpDestructible)
            call SaveDestructableHandleBJ(udg_TmpDestructible, 0, GetHandleIdBJ(udg_TmpHandle), udg_DwarfLumberMillTrees)
        else
            call DoNothing()
        endif
    else
        call IssueTargetDestructableOrder(GetEnumUnit(), "harvest", udg_TmpDestructible)
    endif
endfunction

function Trig_Dwarf_Lumber_Mill_Auto_Return_Actions takes nothing returns nothing
    call ForGroupBJ(udg_DwarfLumberMills, function Trig_Dwarf_Lumber_Mill_Auto_Return_Func001A)
    call PolledWait(5.00)
    call ForGroupBJ(udg_DwarfLumberMills, function Trig_Dwarf_Lumber_Mill_Auto_Return_Func003A)
endfunction

