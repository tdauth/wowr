//===========================================================================
// Trigger: Hero Creation
//===========================================================================
function Trig_Hero_Creation_Conditions takes nothing returns boolean
    if ( not ( IsUnitIdType(GetUnitTypeId(GetTriggerUnit()), UNIT_TYPE_HERO) == true ) ) then
        return false
    endif
    if ( not ( GetUnitTypeId(GetTriggerUnit()) != 'E008' ) ) then
        return false
    endif
    if ( not ( udg_Held_wurde_erschaffen[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_Hero_Creation_Func014A takes nothing returns nothing
    call SetPlayerAllianceBJ(GetOwningPlayer(GetTriggerUnit()), ALLIANCE_SHARED_VISION, true, GetEnumPlayer())
    call SetPlayerAllianceBJ(GetEnumPlayer(), ALLIANCE_SHARED_VISION, true, GetOwningPlayer(GetTriggerUnit()))
endfunction

function Trig_Hero_Creation_Func019Func001C takes nothing returns boolean
    if ( not ( udg_PlayerIsWarlord[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Hero_Creation_Func019C takes nothing returns boolean
    if ( not ( udg_RepickingHero[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] == true ) ) then
        return false
    endif
    if ( not ( udg_HeroFullRepick[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_Hero_Creation_Actions takes nothing returns nothing
    set udg_Held_wurde_erschaffen[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=true
    set udg_Held[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=GetTriggerUnit()
    call PauseUnitBJ(true, udg_Held[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))])
    call SetUnitInvulnerable(udg_Held[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))], true)
    // Hero Counters
    set udg_TmpPlayer=GetOwningPlayer(GetTriggerUnit())
    call ConditionalTriggerExecute(gg_trg_Outland_Taverns_Update_Hero_Counter)
    // Hide Hero Selection
    call FogModifierStop(udg_VisibleModifierVisible[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))])
    call FogModifierStart(udg_VisibleModifierBlackMask[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))])
    call FogModifierStart(udg_VisibleModifierBlackMaskSecret[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))])
    call FogModifierStop(udg_VisibleModifierVisibleSecret[GetConvertedPlayerId(GetEnumPlayer())])
    call ForForce(GetPlayersAllies(GetOwningPlayer(GetTriggerUnit())), function Trig_Hero_Creation_Func014A)
    // Hide Neutral Buildings
    set udg_TmpInteger2=0
    set bj_forLoopAIndex=0
    set bj_forLoopAIndexEnd=udg_NeutralBuildingMax
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set udg_TmpInteger=GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))
        set udg_TmpInteger=Index2D(udg_TmpInteger , udg_TmpInteger2 , 1000)
        call DestroyFogModifier(udg_NeutralBuildingVisibility[udg_TmpInteger])
        set udg_TmpInteger2=( udg_TmpInteger2 + 1 )
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    // Only allow choosing the game mode with the inital hero selection
    if ( Trig_Hero_Creation_Func019C() ) then
        if ( Trig_Hero_Creation_Func019Func001C() ) then
            // Warlord
            set udg_TmpPlayer=GetOwningPlayer(GetTriggerUnit())
            call ConditionalTriggerExecute(gg_trg_Game_Mode_Warlord_Setup)
            // Race Selection Dialog
            set udg_TmpPlayer=GetOwningPlayer(GetTriggerUnit())
            call ConditionalTriggerExecute(gg_trg_Show_Race_Selection_Dialog)
        else
            // Freelancer
            set udg_TmpPlayer=GetOwningPlayer(GetTriggerUnit())
            call ConditionalTriggerExecute(gg_trg_Game_Mode_Freelancer_Setup)
            // Profession Dialog
            set udg_TmpPlayer=GetOwningPlayer(GetTriggerUnit())
            call ConditionalTriggerExecute(gg_trg_Show_Profession_Selection_Dialog)
        endif
    else
        set udg_RepickingHero[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=true
        // Profession Dialog
        set udg_TmpPlayer=GetOwningPlayer(GetTriggerUnit())
        call ConditionalTriggerExecute(gg_trg_Show_Game_Mode_Dialog)
    endif
endfunction

