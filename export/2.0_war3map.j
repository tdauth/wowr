function Trig_Pickpocketing_Conditions takes nothing returns boolean
    if ( not ( GetSpellAbilityId() == 'A084' ) ) then
        return false
    endif
    return true
endfunction

function Trig_Pickpocketing_Func002Func001C takes nothing returns boolean
    if ( not ( udg_TmpInteger == 0 ) ) then
        return false
    endif
    if ( not ( UnitItemInSlotBJ(GetSpellTargetUnit(), GetForLoopIndexA()) != null ) ) then
        return false
    endif
    return true
endfunction

function Trig_Pickpocketing_Func003Func004Func007Func001Func001C takes nothing returns boolean
    if ( not ( udg_TmpInteger == 0 ) ) then
        return false
    endif
    if ( not ( UnitItemInSlotBJ(GetSpellTargetUnit(), GetForLoopIndexA()) != null ) ) then
        return false
    endif
    return true
endfunction

function Trig_Pickpocketing_Func003Func004Func007C takes nothing returns boolean
    if ( not ( udg_TmpReal <= 20.00 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Pickpocketing_Func003Func004C takes nothing returns boolean
    if ( not ( GetOwningPlayer(GetSpellTargetUnit()) != Player(PLAYER_NEUTRAL_AGGRESSIVE) ) ) then
        return false
    endif
    if ( not ( GetOwningPlayer(GetSpellTargetUnit()) != Player(PLAYER_NEUTRAL_PASSIVE) ) ) then
        return false
    endif
    return true
endfunction

function Trig_Pickpocketing_Func003C takes nothing returns boolean
    if ( not ( udg_TmpInteger == 1 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Pickpocketing_Actions takes nothing returns nothing
    set udg_TmpInteger=0
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=6
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ( Trig_Pickpocketing_Func002Func001C() ) then
            set udg_TmpInteger=1
        else
            call DoNothing()
        endif
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    if ( Trig_Pickpocketing_Func003C() ) then
        if ( Trig_Pickpocketing_Func003Func004C() ) then
            set udg_TmpReal=GetRandomReal(0, 100.00)
            set udg_TmpInteger=0
            if ( Trig_Pickpocketing_Func003Func004Func007C() ) then
                set bj_forLoopAIndex=1
                set bj_forLoopAIndexEnd=6
                loop
                    exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                    if ( Trig_Pickpocketing_Func003Func004Func007Func001Func001C() ) then
                        set udg_TmpInteger=1
                        call UnitRemoveItemFromSlotSwapped(GetForLoopIndexA(), GetSpellTargetUnit())
                        call UnitAddItemSwapped(GetLastRemovedItem(), GetTriggerUnit())
                        call DisplayTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), ( "Stole " + ( GetItemName(GetSpellTargetItem()) + "!" ) ))
                    else
                        call DoNothing()
                    endif
                    set bj_forLoopAIndex=bj_forLoopAIndex + 1
                endloop
            else
                call DisplayTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), "TRIGSTR_6713")
            endif
        else
            call IssueImmediateOrderBJ(GetTriggerUnit(), "stop")
            call DisplayTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), "TRIGSTR_6714")
        endif
    else
        call IssueImmediateOrderBJ(GetTriggerUnit(), "stop")
        call DisplayTextToForce(GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())), "TRIGSTR_6722")
    endif
endfunction

