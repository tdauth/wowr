                            set udg_TmpBoolean=true
                        else
                            call DoNothing()
                        endif
                    else
                        call DoNothing()
                    endif
                    set bj_forLoopAIndex=bj_forLoopAIndex + 1
                endloop
            else
                call DoNothing()
            endif
        else
            call DoNothing()
        endif
        // Summon Mercenaries for Freelancers
        if ( Trig_Computer_Auto_Craft_Items_and_Auto_Summon_Mercenaries_Func001Func001Func004C() ) then
            set udg_TmpInteger=1
            set udg_TmpInteger=IMaxBJ(1, ModuloInteger(GetHeroLevel(udg_Held[GetConvertedPlayerId(GetEnumPlayer())]), 10))
            set udg_TmpInteger=IMinBJ(udg_TmpInteger, 10)
            call CreateNUnitsAtLoc(1, ChooseRandomCreepBJ(GetRandomInt(1, udg_TmpInteger)), GetEnumPlayer(), GetUnitLoc(udg_Held[GetConvertedPlayerId(GetEnumPlayer())]), GetRandomDirectionDeg())
            call IssueTargetOrderBJ(GetLastCreatedUnit(), "move", udg_Held[GetConvertedPlayerId(GetEnumPlayer())])
            call h__GroupAddUnitSimple(GetLastCreatedUnit(), udg_AIMercenaries[GetConvertedPlayerId(GetEnumPlayer())])
            call h__GroupAddUnitSimple(GetLastCreatedUnit(), udg_AIMercenariesAll)
        else
