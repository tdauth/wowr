    if ( not ( IsUnitGroupEmptyBJ(udg_GivenUnitsToFreelancer[GetConvertedPlayerId(GetTriggerPlayer())]) == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_Chat_Command_Give_Actions takes nothing returns nothing
    if ( Trig_Chat_Command_Give_Func001C() ) then
        set udg_GivenUnitsToFreelancerPlayer[GetConvertedPlayerId(GetTriggerPlayer())]=S2I(SubStringBJ(GetEventPlayerChatString(), ( StringLength("-give") + 1 ), StringLength(GetEventPlayerChatString())))
        if ( Trig_Chat_Command_Give_Func001Func003C() ) then
            if ( Trig_Chat_Command_Give_Func001Func003Func004C() ) then
                set udg_TmpPlayer=ConvertedPlayer(udg_GivenUnitsToFreelancerPlayer[GetConvertedPlayerId(GetTriggerPlayer())])
                set udg_TmpString=GetPlayerNameColored(udg_TmpPlayer)
                call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), ( "Giving units to player " + ( udg_TmpString + "." ) ))
                set udg_TmpPlayer=GetTriggerPlayer()
                set udg_TmpString=GetPlayerNameColored(udg_TmpPlayer)
                call DisplayTextToForce(GetForceOfPlayer(ConvertedPlayer(udg_GivenUnitsToFreelancerPlayer[GetConvertedPlayerId(GetTriggerPlayer())])), ( "Receiving units from " + ( udg_TmpString + "." ) ))
                set udg_GiveShownSupplyMessage=false
                call ForGroupBJ(udg_GivenUnitsToFreelancer[GetConvertedPlayerId(GetTriggerPlayer())], function Trig_Chat_Command_Give_Func001Func003Func004Func012A)
                // Stats
                set udg_TmpPlayer=ConvertedPlayer(udg_GivenUnitsToFreelancerPlayer[GetConvertedPlayerId(GetTriggerPlayer())])
                call ConditionalTriggerExecute(gg_trg_Chat_Command_Stats_Update_All)
            else
                set udg_TmpPlayer=ConvertedPlayer(udg_GivenUnitsToFreelancerPlayer[GetConvertedPlayerId(GetTriggerPlayer())])
                set udg_TmpString=GetPlayerNameColored(udg_TmpPlayer)
                call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), ( "Cannot give the units to player " + ( udg_TmpString + " since the player is not allied with you." ) ))
            endif
        else
            call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), ( "Invalid player index " + ( I2S(udg_GivenUnitsToFreelancerPlayer[GetConvertedPlayerId(GetTriggerPlayer())]) + ", must be a value between 1 and 24." ) ))
        endif
    else
        call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), "TRIGSTR_9155")
    endif
