function Trig_Game_Type_Selection_Calculate_Actions takes nothing returns nothing
    set udg_TmpInteger=CountPlayersInForceBJ(GetPlayersMatching(Condition(function Trig_Game_Type_Selection_Calculate_Func002002001001)))
    if ( Trig_Game_Type_Selection_Calculate_Func003C() ) then
        if ( Trig_Game_Type_Selection_Calculate_Func003Func001C() ) then
            call PauseTimerBJ(true, udg_GameTypeTimer)
            call DestroyTimerDialogBJ(udg_GameTypeTimerWindow)
        else
            call DoNothing()
        endif
        set udg_GameTypeVoteIsRunning=false
        if ( Trig_Game_Type_Selection_Calculate_Func003Func003C() ) then
            set udg_GameType=udg_GameTypeFast
            call QuestSetTitleBJ(udg_QuestGameType, "TRIGSTR_3938")
            call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_3711")
            set udg_GameTypeXPRateBonus=200.00
            call ForForce(GetPlayersAll(), function Trig_Game_Type_Selection_Calculate_Func003Func003Func006A)
            call SetGameSpeed(MAP_SPEED_FASTEST)
            call LockGameSpeedBJ()
        else
            if ( Trig_Game_Type_Selection_Calculate_Func003Func003Func001C() ) then
                set udg_GameType=udg_GameTypeHardcore
                call QuestSetTitleBJ(udg_QuestGameType, "TRIGSTR_3939")
                call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_3777")
                set udg_GameTypeXPRateBonus=- 60.00
                // Increased Boss Levels
                call ForGroupBJ(udg_Bosses, function Trig_Game_Type_Selection_Calculate_Func003Func003Func001Func007A)
                // Limit Town Halls
                call ForForce(GetPlayersAll(), function Trig_Game_Type_Selection_Calculate_Func003Func003Func001Func009A)
                // Higher HP for Creeps
                call SetPlayerHandicapBJ(Player(PLAYER_NEUTRAL_AGGRESSIVE), 200.00)
            else
                if ( Trig_Game_Type_Selection_Calculate_Func003Func003Func001Func001C() ) then
                    set udg_GameType=udg_GameTypeEasy
                    set udg_GameTypeXPRateBonus=30.00
                    set udg_GameTypeXPRateBonusAI=- 30.00
                    call ForForce(GetPlayersByMapControl(MAP_CONTROL_COMPUTER), function Trig_Game_Type_Selection_Calculate_Func003Func003Func001Func001Func004A)
                    call SetPlayerHandicapBJ(Player(PLAYER_NEUTRAL_AGGRESSIVE), 60.00)
                    call QuestSetTitleBJ(udg_QuestGameType, "TRIGSTR_13794")
                    call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_13795")
                else
                    set udg_GameType=udg_GameTypeNormal
                    call QuestSetTitleBJ(udg_QuestGameType, "TRIGSTR_3940")
                    call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_3712")
                endif
            endif
        endif
        // AI Respawn
        if ( Trig_Game_Type_Selection_Calculate_Func003Func005C() ) then
            call ConditionalTriggerExecute(gg_trg_AI_Respawn_Dialog)
        else
            call ConditionalTriggerExecute(gg_trg_Save_and_Load_Dialog)
        endif
    else
        call DoNothing()
    endif
endfunction

