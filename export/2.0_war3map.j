        endif
        if ( Trig_AI_Specials_Calculate_Func003Func005C() ) then
            set udg_AISpecial=1
            call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_7406")
        else
            call DoNothing()
        endif
        if ( Trig_AI_Specials_Calculate_Func003Func006C() ) then
            set udg_AISpecial=2
            call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_7407")
        else
            call DoNothing()
        endif
        if ( Trig_AI_Specials_Calculate_Func003Func007C() ) then
            set udg_AISpecial=3
            call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_8270")
        else
            call DoNothing()
        endif
        // Remove/disable The Burning Legion
        if ( Trig_AI_Specials_Calculate_Func003Func009C() ) then
            call SetPlayerOnScoreScreenBJ(false, udg_TheBurningLegion)
            call PauseCompAI(udg_TheBurningLegion, true)
            call ForGroupBJ(GetUnitsOfPlayerAll(udg_TheBurningLegion), function Trig_AI_Specials_Calculate_Func003Func009Func003A)
            call CustomDefeatBJ(udg_TheBurningLegion, "TRIGSTR_7181")
        else
            call DoNothing()
        endif
        // Remove/disable The Alliance
        if ( Trig_AI_Specials_Calculate_Func003Func011C() ) then
            call SetPlayerOnScoreScreenBJ(false, udg_TheAlliance)
            call PauseCompAI(udg_TheAlliance, true)
            call ForGroupBJ(GetUnitsOfPlayerAll(udg_TheAlliance), function Trig_AI_Specials_Calculate_Func003Func011Func003A)
            call CustomDefeatBJ(udg_TheAlliance, "TRIGSTR_57826")
        else
            call DoNothing()
        endif
        // Update Stats
        call ConditionalTriggerExecute(gg_trg_Chat_Command_Stats_Create)
        // AI Freelancers
        if ( Trig_AI_Specials_Calculate_Func003Func015C() ) then
            call ConditionalTriggerExecute(gg_trg_AI_Freelancers_Dialog)
        else
            call DoNothing()
        endif
    else
        call DoNothing()
    endif
endfunction

