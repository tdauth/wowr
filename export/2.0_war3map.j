//===========================================================================
// Trigger: Boss Hero Death Event
//
// Execute additional triggers to avoid reaching the OpLimit.
//===========================================================================
function Trig_Boss_Hero_Death_Event_Func002C takes nothing returns boolean
    if ( not ( GetTriggerUnit() == udg_Held[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] ) ) then
        return false
    endif
    return true
endfunction

function Trig_Boss_Hero_Death_Event_Actions takes nothing returns nothing
    // Legendary Artifacts
    if ( Trig_Boss_Hero_Death_Event_Func002C() ) then
        // Drop the legendary artifacts from all heroes including the backpack if the main hero dies
        set bj_forLoopAIndex=0
        set bj_forLoopAIndexEnd=udg_LegendaryItemTypeSize
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            set udg_TmpItemTypeId=udg_LegendaryItemType[GetForLoopIndexA()]
            set udg_TmpRect=udg_LegendaryItemDropRect[GetForLoopIndexA()]
            call ConditionalTriggerExecute(gg_trg_Boss_Hero_Death_Event_Drop_From_Main_Hero)
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
    else
        // Another hero like hero 2 or hero 3 only drops it when it is in his/her inventory
        set bj_forLoopAIndex=0
        set bj_forLoopAIndexEnd=udg_LegendaryItemTypeSize
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            set udg_TmpItemTypeId=udg_LegendaryItemType[GetForLoopIndexA()]
            set udg_TmpRect=udg_LegendaryItemDropRect[GetForLoopIndexA()]
            set udg_TmpUnit=GetDyingUnit()
            call ConditionalTriggerExecute(gg_trg_Boss_Hero_Death_Event_Drop_From_Additional_Hero)
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
    endif
endfunction

