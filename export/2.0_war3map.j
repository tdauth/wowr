    if ( not ( TimerGetRemaining(udg_HandOfGodTimer[GetConvertedPlayerId(udg_TmpPlayer)]) <= 0.00 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Hand_of_God_Cast_Actions takes nothing returns nothing
    set udg_TmpPlayer=GetOwningPlayer(GetTriggerUnit())
    if ( Trig_Hand_of_God_Cast_Func002C() ) then
        set udg_HandOfGod[GetConvertedPlayerId(udg_TmpPlayer)]=true
        call ForGroupBJ(GetUnitsInRangeOfLocMatching(1024.00, GetSpellTargetLoc(), Condition(function Trig_Hand_of_God_Cast_Func002Func004001003)), function Trig_Hand_of_God_Cast_Func002Func004A)
        set udg_HandOfGodLoopVar[GetConvertedPlayerId(udg_TmpPlayer)]=1
        loop
            exitwhen udg_HandOfGodLoopVar[GetConvertedPlayerId(udg_TmpPlayer)] > 5
            set udg_HandOfGodTarget[( ( GetPlayers() * 5 ) + udg_HandOfGodLoopVar[GetConvertedPlayerId(udg_TmpPlayer)] )]=PolarProjectionBJ(GetSpellTargetLoc(), GetRandomReal(0, 500.00), GetRandomDirectionDeg())
            if ( Trig_Hand_of_God_Cast_Func002Func005Func002C() ) then
                call CreateNUnitsAtLoc(1, 'n01H', udg_TmpPlayer, udg_HandOfGodTarget[( ( GetPlayers() * 5 ) + udg_HandOfGodLoopVar[GetConvertedPlayerId(udg_TmpPlayer)] )], GetRandomDirectionDeg())
                call BlzSetUnitIntegerFieldBJ(GetLastCreatedUnit(), UNIT_IF_LEVEL, ( BlzGetUnitIntegerField(GetLastCreatedUnit(), UNIT_IF_LEVEL) + GetPlayerTechCountSimple('R00U', GetOwningPlayer(GetLastCreatedUnit())) ))
            else
                call CreateNUnitsAtLoc(1, 'n01I', udg_TmpPlayer, udg_HandOfGodTarget[( ( GetPlayers() * 5 ) + udg_HandOfGodLoopVar[GetConvertedPlayerId(udg_TmpPlayer)] )], GetRandomDirectionDeg())
                call BlzSetUnitIntegerFieldBJ(GetLastCreatedUnit(), UNIT_IF_LEVEL, ( BlzGetUnitIntegerField(GetLastCreatedUnit(), UNIT_IF_LEVEL) + GetPlayerTechCountSimple('R00U', GetOwningPlayer(GetLastCreatedUnit())) ))
            endif
            call UnitApplyTimedLifeBJ(60, 'BTLF', GetLastCreatedUnit())
            call StartTimerBJ(udg_HandOfGodTimer[GetConvertedPlayerId(GetTriggerPlayer())], false, 180.00)
            call ForGroupBJ(GetUnitsOfPlayerMatching(udg_TmpPlayer, Condition(function Trig_Hand_of_God_Cast_Func002Func005Func005001002)), function Trig_Hand_of_God_Cast_Func002Func005Func005A)
            set udg_HandOfGodLoopVar[GetConvertedPlayerId(udg_TmpPlayer)]=udg_HandOfGodLoopVar[GetConvertedPlayerId(udg_TmpPlayer)] + 1
        endloop
    else
        call BlzEndUnitAbilityCooldown(GetTriggerUnit(), 'A05X')
        call DisplayTextToForce(GetForceOfPlayer(udg_TmpPlayer), "TRIGSTR_3534")
    endif
endfunction

