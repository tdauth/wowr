    call AdjustPlayerStateBJ(udg_TmpInteger, GetOwningPlayer(GetEnumUnit()), PLAYER_STATE_RESOURCE_LUMBER)
    set udg_TmpReal=GetLocationX(PolarProjectionBJ(GetUnitLoc(GetEnumUnit()), 100.00, 0))
    set udg_TmpReal2=GetLocationY(PolarProjectionBJ(GetUnitLoc(GetEnumUnit()), 100.00, 0))
    call ShowLumberTextTagForForce(udg_TmpPlayerGroup , udg_TmpReal , udg_TmpReal2 , udg_TmpInteger)
