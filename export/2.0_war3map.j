        endif
        set udg_AISpecialVoted=true
        if ( Trig_AI_Specials_Calculate_Func003Func004C() ) then
            set udg_AISpecial=0
            call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_7280")
        else
