function Trig_Chat_Command_Hero_Repick_Actions takes nothing returns nothing
    // No repick during hardcore
    if ( Trig_Chat_Command_Hero_Repick_Func002C() ) then
        // No repick during hero selection
        if ( Trig_Chat_Command_Hero_Repick_Func002Func003C() ) then
            // Repick has a time limit
            if ( Trig_Chat_Command_Hero_Repick_Func002Func003Func005C() ) then
                // No repicking with enemies in range except for Theramore
                if ( Trig_Chat_Command_Hero_Repick_Func002Func003Func005Func002C() ) then
                    // No repicking when controlling Illidan
                    if ( Trig_Chat_Command_Hero_Repick_Func002Func003Func005Func002Func002C() ) then
                        // Remove Race Item
                        if ( Trig_Chat_Command_Hero_Repick_Func002Func003Func005Func002Func002Func002C() ) then
                            set bj_forLoopAIndex=0
                            set bj_forLoopAIndexEnd=( udg_Max_Voelker - 1 )
                            loop
                                exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                                if ( Trig_Chat_Command_Hero_Repick_Func002Func003Func005Func002Func002Func002Func001Func001C() ) then
                                    set bj_forLoopBIndex=1
                                    set bj_forLoopBIndexEnd=6
                                    loop
                                        exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
                                        if ( Trig_Chat_Command_Hero_Repick_Func002Func003Func005Func002Func002Func002Func001Func001Func003Func001C() ) then
                                            call RemoveItem(UnitItemInSlotBJ(udg_Held[GetConvertedPlayerId(GetTriggerPlayer())], GetForLoopIndexB()))
                                        else
                                            call DoNothing()
                                        endif
                                        set bj_forLoopBIndex=bj_forLoopBIndex + 1
                                    endloop
                                    call RemoveAllBackpackItemTypesForPlayer(GetPlayerId(GetTriggerPlayer()) , udg_RaceItemType[GetForLoopIndexA()])
                                else
                                    call DoNothing()
                                endif
                                set bj_forLoopAIndex=bj_forLoopAIndex + 1
                            endloop
                        else
                            call DoNothing()
                        endif
                        // Remove Profession Item
                        set bj_forLoopAIndex=0
                        set bj_forLoopAIndexEnd=( udg_Max_Berufe - 1 )
                        loop
                            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                            if ( Trig_Chat_Command_Hero_Repick_Func002Func003Func005Func002Func002Func004Func001C() ) then
                                set bj_forLoopBIndex=1
                                set bj_forLoopBIndexEnd=6
                                loop
                                    exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
                                    if ( Trig_Chat_Command_Hero_Repick_Func002Func003Func005Func002Func002Func004Func001Func003Func001C() ) then
                                        call RemoveItem(UnitItemInSlotBJ(udg_Held[GetConvertedPlayerId(GetTriggerPlayer())], GetForLoopIndexB()))
                                    else
                                        call DoNothing()
                                    endif
                                    set bj_forLoopBIndex=bj_forLoopBIndex + 1
                                endloop
                                call RemoveAllBackpackItemTypesForPlayer(GetPlayerId(GetTriggerPlayer()) , udg_ProfessionItemType[GetForLoopIndexA()])
                            else
                                call DoNothing()
                            endif
                            set bj_forLoopAIndex=bj_forLoopAIndex + 1
                        endloop
                        // Drop all items on Theramore (because of quest items)
                        call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), "TRIGSTR_1254")
                        call SetUnitPositionLoc(udg_Held[GetConvertedPlayerId(GetTriggerPlayer())], GetRectCenter(gg_rct_redirect_forbidden_zone))
                        set bj_forLoopAIndex=1
                        set bj_forLoopAIndexEnd=6
                        loop
                            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                            call UnitRemoveItemSwapped(UnitItemInSlotBJ(udg_Held[GetConvertedPlayerId(GetTriggerPlayer())], GetForLoopIndexA()), udg_Held[GetConvertedPlayerId(GetTriggerPlayer())])
                            call SetItemPositionLoc(GetLastRemovedItem(), GetRectCenter(gg_rct_redirect_forbidden_zone))
                            set bj_forLoopAIndex=bj_forLoopAIndex + 1
                        endloop
