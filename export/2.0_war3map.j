                    call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), "TRIGSTR_4493")
                endif
            else
                call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), ( "The next repick is allowed in " + ( I2S(R2I(TimerGetRemaining(udg_RepickProfessionTimer[GetConvertedPlayerId(GetTriggerPlayer())]))) + " seconds." ) ))
            endif
        else
            call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), "TRIGSTR_1397")
        endif
    else
        call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), "TRIGSTR_1396")
    endif
