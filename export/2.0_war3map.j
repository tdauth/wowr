                set udg_TmpBoolean=false
                set bj_forLoopAIndex=1
                set bj_forLoopAIndexEnd=6
                loop
                    exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                    if ( Trig_Computer_Auto_Craft_Items_and_Auto_Summon_Mercenaries_Func001Func001Func002Func001Func002Func001C() ) then
                        set udg_TmpItemTypeId=GetNextCraftedProfessionItem(GetEnumPlayer())
                        set udg_TmpBoolean2=udg_TmpItemTypeId != 0
                        if ( Trig_Computer_Auto_Craft_Items_and_Auto_Summon_Mercenaries_Func001Func001Func002Func001Func002Func001Func004C() ) then
                            call UnitAddItemByIdSwapped(udg_TmpItemTypeId, udg_Held[GetConvertedPlayerId(GetEnumPlayer())])
                            set udg_TmpBoolean=true
                        else
                            call DoNothing()
                        endif
                    else
                        call DoNothing()
                    endif
                    set bj_forLoopAIndex=bj_forLoopAIndex + 1
                endloop
            else
                call DoNothing()
            endif
            if ( Trig_Computer_Auto_Craft_Items_and_Auto_Summon_Mercenaries_Func001Func001Func002Func002C() ) then
                set udg_TmpBoolean=false
                set bj_forLoopAIndex=1
                set bj_forLoopAIndexEnd=6
                loop
                    exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                    if ( Trig_Computer_Auto_Craft_Items_and_Auto_Summon_Mercenaries_Func001Func001Func002Func002Func002Func001C() ) then
                        set udg_TmpItemTypeId=GetNextCraftedProfession2Item(GetEnumPlayer())
                        set udg_TmpBoolean2=udg_TmpItemTypeId != 0
                        if ( Trig_Computer_Auto_Craft_Items_and_Auto_Summon_Mercenaries_Func001Func001Func002Func002Func002Func001Func004C() ) then
