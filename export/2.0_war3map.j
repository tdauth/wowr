    set udg_Hero[( GetConvertedPlayerId(udg_HeroCreationPlayer) - 1 )]=GetLastCreatedUnit()
    set udg_Held[GetConvertedPlayerId(udg_HeroCreationPlayer)]=GetLastCreatedUnit()
    call SetHeroXP(udg_Held[GetConvertedPlayerId(udg_HeroCreationPlayer)], udg_CharacterStartXP[GetConvertedPlayerId(udg_HeroCreationPlayer)], true)
    call SetUnitInvulnerable(udg_Held[GetConvertedPlayerId(udg_HeroCreationPlayer)], true)
    call SelectUnitForPlayerSingle(GetLastCreatedUnit(), GetOwningPlayer(GetTriggerUnit()))
    call PanCameraToTimedLocForPlayer(udg_HeroCreationPlayer, GetUnitLoc(udg_Held[GetConvertedPlayerId(udg_HeroCreationPlayer)]), 0)
    call RemoveUnit(GetTriggerUnit())
    call CreateRucksackForPlayer(GetPlayerId(udg_HeroCreationPlayer))
    call DisplayTextToForce(GetForceOfPlayer(udg_HeroCreationPlayer), "TRIGSTR_1170")
endfunction

