function Trig_AI_Respawn_Calculate_Actions takes nothing returns nothing
    set udg_TmpInteger=CountPlayersInForceBJ(GetPlayersMatching(Condition(function Trig_AI_Respawn_Calculate_Func002002001001)))
    if ( Trig_AI_Respawn_Calculate_Func003C() ) then
        set udg_AIFreelancersVoteIsRunning=false
        if ( Trig_AI_Respawn_Calculate_Func003Func002C() ) then
            call PauseTimerBJ(true, udg_AIRespawnTimer)
            call DestroyTimerDialogBJ(udg_AIRespawnTimerWindow)
        else
            call DoNothing()
        endif
        set udg_AIRespawnVoted=true
        if ( Trig_AI_Respawn_Calculate_Func003Func004C() ) then
            set udg_AIRespawn=false
            call QuestSetTitleBJ(udg_QuestAI, "TRIGSTR_6211")
            call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_6213")
        else
            set udg_AIRespawn=true
            call QuestSetTitleBJ(udg_QuestAI, "TRIGSTR_6214")
            call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_6215")
        endif
        // AI Specials
        if ( Trig_AI_Respawn_Calculate_Func003Func006C() ) then
            call ConditionalTriggerExecute(gg_trg_AI_Specials_Dialog)
        else
            call DoNothing()
        endif
    else
        call DoNothing()
    endif
endfunction

