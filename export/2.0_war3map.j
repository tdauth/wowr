    call DisplayTextToForce(GetPlayersAll(), ( "The hash of your player name is: " + I2S(StringHashBJ(GetPlayerName(GetTriggerPlayer()))) ))
    set udg_TmpString=ConvertDecimalNumberToSaveCodeSegment(2000) + ConvertDecimalNumberToSaveCodeSegment(2000) + ConvertDecimalNumberToSaveCodeSegment(2000)
    set udg_TmpInteger=ConvertSaveCodeSegmentIntoDecimalNumberFromSaveCode(udg_TmpString , 0) + ConvertSaveCodeSegmentIntoDecimalNumberFromSaveCode(udg_TmpString , 1) + ConvertSaveCodeSegmentIntoDecimalNumberFromSaveCode(udg_TmpString , 2)
    call DisplayTextToForce(GetPlayersAll(), ( "The input 2000 + 2000 + 2000 should be " + I2S(udg_TmpInteger) ))
    set udg_TmpString=ConvertDecimalNumberToSaveCodeSegment(266132599)
    set udg_TmpInteger=ConvertSaveCodeSegmentIntoDecimalNumberFromSaveCode(udg_TmpString , 0)
    call DisplayTextToForce(GetPlayersAll(), ( "The input 266132599 should be " + I2S(udg_TmpInteger) ))
    call DisplayTextToForce(GetPlayersAll(), ( "This is the generated string: " + udg_TmpString ))
    set udg_TmpString=ConvertDecimalNumberToSaveCodeSegment(266132500)
    set udg_TmpInteger=ConvertSaveCodeSegmentIntoDecimalNumberFromSaveCode(udg_TmpString , 0)
    call DisplayTextToForce(GetPlayersAll(), ( "The input 266132500 should be " + I2S(udg_TmpInteger) ))
    call DisplayTextToForce(GetPlayersAll(), ( "This is the generated string: " + udg_TmpString ))
    set udg_TmpString=ConvertDecimalNumberToSaveCodeSegment(GetMaxSaveCodeDigits())
    set udg_TmpInteger=GetMaxSaveCodeDigits()
    call DisplayTextToForce(GetPlayersAll(), ( ( I2S(udg_TmpInteger) + " converted: " ) + udg_TmpString ))
    set udg_TmpInteger=1010975283
    set udg_TmpString=ConvertDecimalNumberToSaveCodeSegment(udg_TmpInteger)
    call DisplayTextToForce(GetPlayersAll(), ( ( I2S(udg_TmpInteger) + " converted: " ) + udg_TmpString ))
    set udg_TmpInteger2=ConvertSaveCodeSegmentIntoDecimalNumberFromSaveCode(udg_TmpString , 0)
    call DisplayTextToForce(GetPlayersAll(), ( ( I2S(udg_TmpInteger) + " converted back: " ) + I2S(udg_TmpInteger2) ))
    call DisplayTextToForce(GetPlayersAll(), ( "Basic division 63185955/16: " + I2S(( 63185956 / 16 )) ))
    call DisplayTextToForce(GetPlayersAll(), ( "Basic 63185955 mod 16: " + I2S(ModuloInteger(63185956, 16)) ))
    call DisplayTextToForce(GetPlayersAll(), ( "Basic 63185955 - 63185952: " + I2S(( 63185956 - 63185952 )) ))
    set udg_TmpString=ConvertDecimalNumberToSaveCodeSegment(1054796325)
    set udg_TmpInteger=ConvertSaveCodeSegmentIntoDecimalNumberFromSaveCode(udg_TmpString , 0)
    call DisplayTextToForce(GetPlayersAll(), ( "The input 1054796325 should be " + I2S(udg_TmpInteger) ))
    call DisplayTextToForce(GetPlayersAll(), ( "This is the generated string: " + udg_TmpString ))
    set udg_TmpString=ConvertSaveCodeToObfuscatedVersion("ABC" , 4)
    call DisplayTextToForce(GetPlayersAll(), ( "This is the obfuscated save code from ABC: " + udg_TmpString ))
    set udg_TmpString2=ConvertSaveCodeFromObfuscatedVersion(udg_TmpString , 4)
    call DisplayTextToForce(GetPlayersAll(), ( "This is the unobfuscated save code from ABC: " + udg_TmpString2 ))
