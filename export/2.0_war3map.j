function Trig_Profession_Selection_Func001Func001C takes nothing returns boolean
    if ( not ( udg_ProfessionDialogButton[( ( ( GetConvertedPlayerId(GetTriggerPlayer()) - 1 ) * udg_Max_Berufe ) + udg_IntegerE )] == GetClickedButtonBJ() ) ) then
        return false
    endif
    return true
endfunction

function Trig_Profession_Selection_Actions takes nothing returns nothing
    set udg_IntegerE=0
    loop
        exitwhen udg_IntegerE > ( udg_Max_Berufe - 1 )
        if ( Trig_Profession_Selection_Func001Func001C() ) then
            call DialogClearBJ(udg_ProfessionDialog[GetConvertedPlayerId(GetTriggerPlayer())])
            // Create the Spell Book
            call PauseUnitBJ(false, udg_Held[GetConvertedPlayerId(GetTriggerPlayer())])
            call SetUnitInvulnerable(udg_Held[GetConvertedPlayerId(GetTriggerPlayer())], false)
            // Select Profession
            set udg_TmpPlayer=GetTriggerPlayer()
            set udg_TmpInteger=udg_IntegerE
            call ConditionalTriggerExecute(gg_trg_Select_Profession)
            if ( Trig_Profession_Selection_Func001Func001Func010C() ) then
                // Repick Timer
                call StartTimerBJ(udg_RepickProfessionTimer[GetConvertedPlayerId(GetTriggerPlayer())], false, udg_RepickProfessionTime)
                set udg_HeroRepickProfession[GetConvertedPlayerId(GetTriggerPlayer())]=false
            else
                // Stop the Hero
                call IssueImmediateOrderBJ(udg_Held[GetConvertedPlayerId(GetTriggerPlayer())], "stop")
                // Repick Timers
                call StartTimerBJ(udg_RepickTimer[GetConvertedPlayerId(GetTriggerPlayer())], false, 0.00)
                call StartTimerBJ(udg_RepickProfessionTimer[GetConvertedPlayerId(GetTriggerPlayer())], false, udg_RepickProfessionTime)
                // Pan
                call PanCameraToTimedLocForPlayer(GetTriggerPlayer(), GetUnitLoc(udg_Held[GetConvertedPlayerId(GetTriggerPlayer())]), 0)
            endif
        else
            call DoNothing()
        endif
        set udg_IntegerE=udg_IntegerE + 1
    endloop
endfunction

