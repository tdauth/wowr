    if ( not ( ( GetPlayerState(ConvertedPlayer(udg_GivenUnitsToFreelancerPlayer[GetConvertedPlayerId(GetTriggerPlayer())]), PLAYER_STATE_RESOURCE_FOOD_USED) + GetUnitFoodUsed(GetEnumUnit()) ) <= GetPlayerState(ConvertedPlayer(udg_GivenUnitsToFreelancerPlayer[GetConvertedPlayerId(GetTriggerPlayer())]), PLAYER_STATE_RESOURCE_FOOD_CAP) ) ) then
