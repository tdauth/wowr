function Trig_AI_Specials_Calculate_Actions takes nothing returns nothing
    set udg_TmpInteger=CountPlayersInForceBJ(GetPlayersMatching(Condition(function Trig_AI_Specials_Calculate_Func002002001001)))
    if ( Trig_AI_Specials_Calculate_Func003C() ) then
        set udg_AISpecialVoteIsRunning=false
