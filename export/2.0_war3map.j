    set bj_forLoopAIndex=4
    set bj_forLoopAIndexEnd=100
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call BlackArrowAddAbility('ANba' , GetForLoopIndexA() , 'ndr3' , 1 , 80.0 , 0.0 , 2.0 , 'BNdm')
