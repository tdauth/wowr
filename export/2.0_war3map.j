endfunction

function Trig_Game_Settings_Calculate_Func003Func005C takes nothing returns boolean
    if ( not ( udg_GameSettingsVotesNo > udg_GameSettingsVotesYes ) ) then
        return false
    endif
    return true
endfunction

function Trig_Game_Settings_Calculate_Func003C takes nothing returns boolean
    if ( not ( ( udg_GameSettingsVotesNo + udg_GameSettingsVotesYes ) == udg_TmpInteger ) ) then
        return false
    endif
    return true
endfunction

function Trig_Game_Settings_Calculate_Actions takes nothing returns nothing
    set udg_TmpInteger=CountPlayersInForceBJ(GetPlayersMatching(Condition(function Trig_Game_Settings_Calculate_Func002002001001)))
    if ( Trig_Game_Settings_Calculate_Func003C() ) then
        call PauseTimerBJ(true, udg_GameSettingsTimer)
        call DestroyTimerDialogBJ(udg_GameSettingsTimerWindow)
        set udg_GameSettingsVoted=true
        set udg_GameSettingsVoteIsRunning=false
        if ( Trig_Game_Settings_Calculate_Func003Func005C() ) then
            set udg_GameSettingsUseDefault=true
            call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_11457")
            set udg_TmpInteger=CountPlayersInForceBJ(GetPlayersMatching(Condition(function Trig_Game_Settings_Calculate_Func003Func005Func004002001001)))
            set udg_GameTypeVotesNormal=udg_TmpInteger
            call ConditionalTriggerExecute(gg_trg_Game_Type_Selection_Calculate)
            set udg_AIRespawnVotesNo=udg_TmpInteger
            call ConditionalTriggerExecute(gg_trg_AI_Respawn_Calculate)
            set udg_AISpecialVotesNone=udg_TmpInteger
            // Changes TmpInteger:
            call ConditionalTriggerExecute(gg_trg_AI_Specials_Calculate)
            set udg_TmpInteger=CountPlayersInForceBJ(GetPlayersMatching(Condition(function Trig_Game_Settings_Calculate_Func003Func005Func012002001001)))
            set udg_AIFreelancersVotesBoth=udg_TmpInteger
            call ConditionalTriggerExecute(gg_trg_AI_Freelancers_Calculate)
            call ConditionalTriggerExecute(gg_trg_Computer_Init_No_AI)
            set udg_VIPVotesYes=udg_TmpInteger
            call ConditionalTriggerExecute(gg_trg_VIP_Calculate_Votes)
            set udg_VictoryVotesNo=udg_TmpInteger
            call ConditionalTriggerExecute(gg_trg_Victory_Calculate)
            call ConditionalTriggerExecute(gg_trg_Enable_Location_Portals)
        else
            set udg_GameSettingsUseDefault=false
            call ConditionalTriggerExecute(gg_trg_Game_Type_Selection_Dialog)
        endif
    else
        call DoNothing()
    endif
endfunction

