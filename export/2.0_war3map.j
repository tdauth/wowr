    call SetPlayerAllianceStateBJ(udg_TheBurningLegion, GetOwningPlayer(GetTriggerUnit()), bj_ALLIANCE_ALLIED)
    set udg_AllianceWithTheBurningLegionOn[GetConvertedPlayerId(GetTriggerPlayer())]=true
    set udg_TmpString=GetPlayerNameColored(GetOwningPlayer(GetTriggerUnit()))
