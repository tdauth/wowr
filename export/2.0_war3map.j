            else
                // Enable research No Second Hero to allow to pick a third hero
                call SetPlayerTechResearchedSwap('R028', 1, udg_TmpPlayer)
                if ( Trig_Outland_Taverns_Update_Hero_Counter_Func001Func003Func001Func004C() ) then
                    call SetPlayerStateBJ(udg_TmpPlayer, PLAYER_STATE_RESOURCE_HERO_TOKENS, 1)
                else
                    call SetPlayerStateBJ(udg_TmpPlayer, PLAYER_STATE_RESOURCE_HERO_TOKENS, 0)
                endif
            endif
        endif
    else
        if ( Trig_Outland_Taverns_Update_Hero_Counter_Func001Func001C() ) then
            call SetPlayerMaxHeroesAllowed(2, udg_TmpPlayer)
            if ( Trig_Outland_Taverns_Update_Hero_Counter_Func001Func001Func005C() ) then
