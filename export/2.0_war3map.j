        exitwhen ( i == bj_MAX_PLAYERS )
        if ( StringLength(GetPlayerName(Player(i))) > 0 and CompressedAbsStringHash(GetPlayerName(Player(i))) == playerNameHash ) then
            return GetPlayerName(Player(i))
        endif
        set i=i + 1
    endloop

    return "Unknown"
