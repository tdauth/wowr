            call SetPlayerStateBJ(udg_TmpPlayer, PLAYER_STATE_RESOURCE_HERO_TOKENS, 2)
            call SetPlayerTechResearchedSwap('R028', 1, udg_TmpPlayer)
        else
            if ( Trig_Outland_Taverns_Update_Hero_Counter_Func001Func003Func001C() ) then
                call SetPlayerStateBJ(udg_TmpPlayer, PLAYER_STATE_RESOURCE_HERO_TOKENS, 1)
