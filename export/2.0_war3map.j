function Trig_Dwarf_Lumber_Mill_Auto_Return_Func003Func004Func002C takes nothing returns boolean
    if ( ( udg_TmpDestructible == null ) ) then
        return true
    endif
    if ( ( IsDestructableDeadBJ(udg_TmpDestructible) == true ) ) then
        return true
    endif
    if ( ( DistanceBetweenPoints(GetUnitLoc(GetEnumUnit()), GetDestructableLoc(udg_TmpDestructible)) > 4000.00 ) ) then
        return true
    endif
    return false
endfunction

