    call SetPlayerAllianceStateBJ(udg_TmpPlayer, GetEnumPlayer(), bj_ALLIANCE_UNALLIED)
    call SetPlayerAllianceStateBJ(GetEnumPlayer(), udg_TmpPlayer, bj_ALLIANCE_UNALLIED)
endfunction

function Trig_Chat_Command_Update_Passive_Func001Func014Func001C takes nothing returns boolean
    if ( not ( udg_BossesPlayer != GetEnumPlayer() ) ) then
        return false
    endif
    if ( not ( Player(PLAYER_NEUTRAL_AGGRESSIVE) != GetEnumPlayer() ) ) then
        return false
    endif
    return true
endfunction

function Trig_Chat_Command_Update_Passive_Func001Func014A takes nothing returns nothing
    if ( Trig_Chat_Command_Update_Passive_Func001Func014Func001C() ) then
        call ForceAddPlayerSimple(GetEnumPlayer(), udg_PlayerPassiveModeEnemies[GetConvertedPlayerId(udg_TmpPlayer)])
        call SetPlayerAllianceStateBJ(udg_TmpPlayer, GetEnumPlayer(), bj_ALLIANCE_NEUTRAL)
        call SetPlayerAllianceStateBJ(GetEnumPlayer(), udg_TmpPlayer, bj_ALLIANCE_NEUTRAL)
    else
        call DoNothing()
    endif
endfunction

function Trig_Chat_Command_Update_Passive_Func001C takes nothing returns boolean
    if ( not ( udg_PlayerIsPassive[GetConvertedPlayerId(udg_TmpPlayer)] == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Chat_Command_Update_Passive_Actions takes nothing returns nothing
    if ( Trig_Chat_Command_Update_Passive_Func001C() ) then
        call DisableTrigger(gg_trg_Computer_Alliance_Change)
        call SetPlayerAllianceStateBJ(udg_TmpPlayer, udg_TheBurningLegion, bj_ALLIANCE_NEUTRAL)
        call SetPlayerAllianceStateBJ(udg_TmpPlayer, udg_TheAlliance, bj_ALLIANCE_NEUTRAL)
        call SetPlayerAllianceStateBJ(udg_TheBurningLegion, udg_TmpPlayer, bj_ALLIANCE_NEUTRAL)
        call SetPlayerAllianceStateBJ(udg_TheAlliance, udg_TmpPlayer, bj_ALLIANCE_NEUTRAL)
        call ForceClear(udg_PlayerPassiveModeEnemies[GetConvertedPlayerId(udg_TmpPlayer)])
        call ForForce(GetPlayersEnemies(udg_TmpPlayer), function Trig_Chat_Command_Update_Passive_Func001Func014A)
        call EnableTrigger(gg_trg_Computer_Alliance_Change)
    else
        call DisableTrigger(gg_trg_Computer_Alliance_Change)
        call SetPlayerAllianceStateBJ(udg_TmpPlayer, udg_TheBurningLegion, bj_ALLIANCE_UNALLIED)
        call SetPlayerAllianceStateBJ(udg_TmpPlayer, udg_TheAlliance, bj_ALLIANCE_UNALLIED)
        call SetPlayerAllianceStateBJ(udg_TheBurningLegion, udg_TmpPlayer, bj_ALLIANCE_ALLIED)
        call SetPlayerAllianceStateBJ(udg_TheAlliance, udg_TmpPlayer, bj_ALLIANCE_ALLIED)
        call ForForce(udg_PlayerPassiveModeEnemies[GetConvertedPlayerId(udg_TmpPlayer)], function Trig_Chat_Command_Update_Passive_Func001Func007A)
        call EnableTrigger(gg_trg_Computer_Alliance_Change)
    endif
endfunction

