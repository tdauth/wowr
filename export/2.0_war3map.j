function Trig_Human_Quest_4_Discover_and_Complete_Actions takes nothing returns nothing
    // Discover
    if ( Trig_Human_Quest_4_Discover_and_Complete_Func002C() ) then
        // Menschen-Quest 4
        call QuestSetDiscoveredBJ(udg_HumanQuest[4], true)
        call FlashQuestDialogButtonBJ()
        call QuestMessageBJ(GetPlayersAll(), bj_QUESTMESSAGE_DISCOVERED, "TRIGSTR_3928")
        // Quest-Daten
        call CreateLeaderboardBJ(GetPlayersByMapControl(MAP_CONTROL_USER), "TRIGSTR_3932")
        set udg_QuestLeaderboard[1]=GetLastCreatedLeaderboard()
        call ForForce(GetPlayersMatching(Condition(function Trig_Human_Quest_4_Discover_and_Complete_Func002Func009001001)), function Trig_Human_Quest_4_Discover_and_Complete_Func002Func009A)
        call LeaderboardDisplayBJ(true, udg_QuestLeaderboard[1])
        call EnableTrigger(gg_trg_Human_Quest_4_Kill_Undead_Hero)
        call EnableTrigger(gg_trg_Crusade_On)
        call EnableTrigger(gg_trg_Crusade_Off)
        call PlaySoundBJ(gg_snd_Menschen_Quest_4_Aufruf_zum_Kreuzzug)
        call WaitForSoundBJ(gg_snd_Menschen_Quest_4_Aufruf_zum_Kreuzzug, 0)
        call PlayThematicMusicBJ("PursuitTheme")
    else
        call DoNothing()
    endif
    // Complete
    if ( Trig_Human_Quest_4_Discover_and_Complete_Func004C() ) then
        call DisableTrigger(GetTriggeringTrigger())
        // Menschen-Quest 4 abgeschlossen
        call QuestSetCompletedBJ(udg_HumanQuest[4], true)
        call FlashQuestDialogButtonBJ()
        call QuestMessageBJ(GetPlayersAll(), bj_QUESTMESSAGE_COMPLETED, "TRIGSTR_6255")
        call DestroyLeaderboardBJ(udg_QuestLeaderboard[1])
        call DestroyEffectBJ(udg_Quest_Effekt[udg_Quest_Geber_Anduin])
        call RemoveItem(GetItemOfTypeFromUnitBJ(GetTriggerUnit(), 'I01D'))
        set udg_QuestsCompletedByPlayer[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( udg_QuestsCompletedByPlayer[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] + 1 )
        // Quest-Belohnung
        call UnitAddItemByIdSwapped('I01C', GetTriggerUnit())
    else
        call DoNothing()
    endif
endfunction

