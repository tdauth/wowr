//===========================================================================
function Trig_Paladins_leaderboard_update_Func001C takes nothing returns boolean
    if ( ( GetUnitTypeId(GetTriggerUnit()) == 'Hpal' ) ) then
        return true
    endif
    if ( ( GetUnitTypeId(GetTriggerUnit()) == 'H01P' ) ) then
        return true
    endif
    if ( ( GetUnitTypeId(GetTriggerUnit()) == 'Hpb1' ) ) then
        return true
    endif
    if ( ( GetUnitTypeId(GetTriggerUnit()) == 'Hmgd' ) ) then
        return true
    endif
    return false
endfunction

function Trig_Paladins_leaderboard_update_Conditions takes nothing returns boolean
    if ( not Trig_Paladins_leaderboard_update_Func001C() ) then
        return false
    endif
    if ( not ( GetOwningPlayer(GetKillingUnitBJ()) != Player(PLAYER_NEUTRAL_AGGRESSIVE) ) ) then
        return false
    endif
    if ( not ( GetOwningPlayer(GetKillingUnitBJ()) != GetOwningPlayer(GetDyingUnit()) ) ) then
        return false
    endif
    if ( not ( IsPlayerEnemy(GetOwningPlayer(GetTriggerUnit()), GetOwningPlayer(GetKillingUnitBJ())) == true ) ) then
        return false
    endif
    if ( not ( IsPlayerEnemy(GetOwningPlayer(GetKillingUnitBJ()), GetOwningPlayer(GetTriggerUnit())) == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Paladins_leaderboard_update_Func005C takes nothing returns boolean
    if ( not ( udg_palakills[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))] == 5 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Paladins_leaderboard_update_Actions takes nothing returns nothing
    set udg_palakills[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))]=( udg_palakills[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))] + 1 )
    call LeaderboardSetPlayerItemValueBJ(GetOwningPlayer(GetKillingUnitBJ()), udg_PaladinsSlain, udg_palakills[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))])
    call LeaderboardSortItemsBJ(udg_PaladinsSlain, bj_SORTTYPE_SORTBYVALUE, false)
    if ( Trig_Paladins_leaderboard_update_Func005C() ) then
        call DisableTrigger(gg_trg_Paladins_leaderboard_update)
        call LeaderboardDisplayBJ(false, udg_PaladinsSlain)
        set udg_TmpString=GetPlayerNameColored(GetOwningPlayer(GetKillingUnit()))
        call DisplayTimedTextToForce(GetPlayersAll(), 30, ( udg_TmpString + "  has first slain 5 Paladins and will be given a reward by Sylvanas Windrunner in the Undercity !" ))
        call UnitRemoveItemFromSlotSwapped(6, GetKillingUnitBJ())
        call UnitAddItemByIdSwapped('I01W', GetKillingUnitBJ())
        call SetItemInvulnerableBJ(GetLastCreatedItem(), true)
        call EnableTrigger(gg_trg_ForsakenQuest_3b)
        call DisableTrigger(gg_trg_Paladins_on)
        call EnableTrigger(gg_trg_Paladins_off)
        call PlaySoundBJ(gg_snd_GameFound)
        call WaitForSoundBJ(gg_snd_GameFound, 0)
        call PlayThematicMusicBJ("SadMystery")
        call PingMinimapLocForForce(GetPlayersAll(), GetRectCenter(gg_rct_Quest_Sylvanas), 5.00)
    else
        call DoNothing()
    endif
endfunction

