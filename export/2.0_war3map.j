function Trig_Orc_Quest_4_Kill_Centaur_Khan_Conditions takes nothing returns boolean
    if ( not ( GetUnitTypeId(GetTriggerUnit()) == 'ncnk' ) ) then
        return false
    endif
    if ( not ( GetPlayerController(GetOwningPlayer(GetKillingUnitBJ())) == MAP_CONTROL_USER ) ) then
        return false
    endif
    return true
endfunction

function Trig_Orc_Quest_4_Kill_Centaur_Khan_Func005C takes nothing returns boolean
    if ( not ( udg_Orc_Quest_4_Skill_Wert[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))] >= 7 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Orc_Quest_4_Kill_Centaur_Khan_Actions takes nothing returns nothing
    // Increase Khan kill counter
    set udg_Orc_Quest_4_Skill_Wert[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))]=( udg_Orc_Quest_4_Skill_Wert[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))] + 1 )
    call LeaderboardSetPlayerItemValueBJ(GetOwningPlayer(GetKillingUnitBJ()), udg_QuestLeaderboard[2], udg_Orc_Quest_4_Skill_Wert[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))])
    call LeaderboardSortItemsBJ(udg_QuestLeaderboard[2], bj_SORTTYPE_SORTBYVALUE, false)
    if ( Trig_Orc_Quest_4_Kill_Centaur_Khan_Func005C() ) then
        call DisableTrigger(GetTriggeringTrigger())
        call FlashQuestDialogButtonBJ()
        call QuestMessageBJ(GetPlayersAll(), bj_QUESTMESSAGE_UPDATED, "TRIGSTR_6224")
        call AddSpecialEffectTargetUnitBJ("overhead", gg_unit_Ocbh_0011, "Abilities\\Spells\\Other\\TalkToMe\\TalkToMe.mdl")
        set udg_Quest_Effekt[udg_Quest_Geber_Cairne]=GetLastCreatedEffectBJ()
        call PlayThematicMusicBJ("OrcTheme")
        call PingMinimapLocForForce(GetPlayersAll(), GetRectCenter(gg_rct_Orc_Quest_2_entdecken_und_abschliessen), 5.00)
        call DisableTrigger(gg_trg_Centaurs_On)
        call DisableTrigger(gg_trg_Centaurs_Off)
    else
        call DoNothing()
    endif
endfunction

