function Trig_Funny_Bunny_Drop_Egg_Func004A takes nothing returns nothing
    if ( Trig_Funny_Bunny_Drop_Egg_Func004Func001C() ) then
        set udg_TmpInteger=1
        if ( Trig_Funny_Bunny_Drop_Egg_Func004Func001Func002C() ) then
            set udg_TmpInteger=( udg_TmpInteger + GetUnitAbilityLevelSwapped('A0G8', GetEnumUnit()) )
        else
        endif
        set bj_forLoopAIndex=1
        set bj_forLoopAIndexEnd=udg_TmpInteger
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            call CreateItemLoc('I05I', GetUnitLoc(GetEnumUnit()))
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
    else
        call DoNothing()
    endif
endfunction

