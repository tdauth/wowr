        return false
    endif
    if ( not ( GetPlayerTechCountSimple('R00U', Player(PLAYER_NEUTRAL_AGGRESSIVE)) < 1000 ) ) then
        return false
    endif
    if ( not ( GetOwningPlayer(GetTriggerUnit()) != udg_TheBurningLegion ) ) then
