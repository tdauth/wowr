    call CreateNUnitsAtLoc(1, GetUnitTypeId(udg_TmpUnit), udg_TmpPlayer, GetRectCenter(gg_rct_Evolution_Dummy_Area), bj_UNIT_FACING)
    call UnitAddAbilityBJ('Amim', GetLastCreatedUnit())
    call UnitRemoveBuffsBJ(bj_REMOVEBUFFS_ALL, GetLastCreatedUnit())
    call ShowUnitHide(GetLastCreatedUnit())
    call PauseUnitBJ(true, GetLastCreatedUnit())
    set udg_TmpInteger3=GetEvolutionLevelOfPlayer(udg_TmpPlayer)
    set udg_TmpInteger=( udg_TmpInteger3 / 10 )
    set udg_TmpInteger=( udg_TmpInteger + BlzGetUnitIntegerField(GetLastCreatedUnit(), UNIT_IF_LEVEL) )
    set udg_TmpInteger2=( udg_TmpInteger3 / 10 )
    set udg_TmpReal=( I2R(udg_TmpInteger2) + BlzGetUnitRealField(GetLastCreatedUnit(), UNIT_RF_DEFENSE) )
    call ConditionalTriggerExecute(gg_trg_Evolution_Set_Unit_Level_And_Defense)
    call RemoveUnit(GetLastCreatedUnit())
endfunction

