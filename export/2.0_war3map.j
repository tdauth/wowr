                                    set bj_forLoopBIndex=bj_forLoopBIndex + 1
                                endloop
                                call RemoveAllBackpackItemTypesForPlayer(GetPlayerId(GetTriggerPlayer()) , udg_ProfessionItemType[GetForLoopIndexA()])
                            else
                                call DoNothing()
                            endif
                            set bj_forLoopAIndex=bj_forLoopAIndex + 1
                        endloop
                        set udg_PlayerProfession[GetConvertedPlayerId(GetTriggerPlayer())]=udg_ProfessionNone
                        set udg_HeroRepickProfession[GetConvertedPlayerId(GetTriggerPlayer())]=true
                        set udg_TmpPlayer=GetTriggerPlayer()
                        call ConditionalTriggerExecute(gg_trg_Show_Profession_Selection_Dialog)
                    else
