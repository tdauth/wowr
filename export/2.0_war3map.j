    if ( Trig_Chat_Command_Give_Func001Func003Func004Func012Func001C() ) then
        call SetUnitOwner(GetEnumUnit(), ConvertedPlayer(udg_GivenUnitsToFreelancerPlayer[GetConvertedPlayerId(GetTriggerPlayer())]), true)
        call PingMinimapLocForForce(GetForceOfPlayer(ConvertedPlayer(udg_GivenUnitsToFreelancerPlayer[GetConvertedPlayerId(GetTriggerPlayer())])), GetUnitLoc(GetEnumUnit()), 5.00)
    else
        if ( Trig_Chat_Command_Give_Func001Func003Func004Func012Func001Func001C() ) then
            call DisplayTextToForce(GetForceOfPlayer(GetTriggerPlayer()), ( "Cannot give unit " + ( GetUnitName(GetEnumUnit()) + " to the other player since it would exceed his/her food capacity." ) ))
