function Trig_Builder_Hideout_10_Conditions takes nothing returns boolean
    if ( not ( GetUnitTypeId(GetConstructedStructure()) == 'h00J' ) ) then
        return false
    endif
    return true
endfunction

function Trig_Builder_Hideout_10_Func001C takes nothing returns boolean
    if ( not ( udg_HideoutRule == 1 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Builder_Hideout_10_Actions takes nothing returns nothing
    if ( Trig_Builder_Hideout_10_Func001C() ) then
        call DisableTrigger( gg_trg_Defeat_Hideout )
        call KillUnit( GetConstructedStructure() )
        call PlaySoundBJ( gg_snd_MaelstromDeath )
        call DisplayTimedTextToForce( GetPlayersAll(), 30, "TRIGSTR_1425" )
        call EnableTrigger( gg_trg_Defeat_Hideout )
    else
        call KillUnit( GetConstructedStructure() )
        call PlaySoundBJ( gg_snd_MaelstromDeath )
        call DisplayTimedTextToForce( GetPlayersAll(), 30, "TRIGSTR_1424" )
    endif
endfunction

