//===========================================================================
function Trig_Paladins_leaderboard_update_Conditions takes nothing returns boolean
    if ( not ( GetUnitTypeId(GetDyingUnit()) == 'Hpal' ) ) then
        return false
    endif
    if ( not ( GetUnitTypeId(GetKillingUnitBJ()) != 'Hpal' ) ) then
        return false
    endif
    if ( not ( GetOwningPlayer(GetKillingUnitBJ()) != Player(11) ) ) then
        return false
    endif
    if ( not ( GetOwningPlayer(GetKillingUnitBJ()) != Player(PLAYER_NEUTRAL_AGGRESSIVE) ) ) then
        return false
    endif
    if ( not ( GetOwningPlayer(GetKillingUnitBJ()) != GetOwningPlayer(GetDyingUnit()) ) ) then
        return false
    endif
    return true
endfunction

function Trig_Paladins_leaderboard_update_Func007002 takes nothing returns nothing
    call LeaderboardSetPlayerItemValueBJ( GetEnumPlayer(), udg_PaladinsSlain, udg_palakills[GetConvertedPlayerId(GetEnumPlayer())] )
endfunction

function Trig_Paladins_leaderboard_update_Func009Func003002 takes nothing returns nothing
    call LeaderboardDisplayBJ( false, udg_PaladinsSlain )
endfunction

function Trig_Paladins_leaderboard_update_Func009C takes nothing returns boolean
    if ( not ( udg_palakills[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))] == 5 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Paladins_leaderboard_update_Actions takes nothing returns nothing
    set udg_palakills[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))] = ( udg_palakills[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))] + 1 )
    call ForForce( GetPlayersAll(), function Trig_Paladins_leaderboard_update_Func007002 )
    call LeaderboardSortItemsBJ( udg_PaladinsSlain, bj_SORTTYPE_SORTBYVALUE, false )
    if ( Trig_Paladins_leaderboard_update_Func009C() ) then
        call DisableTrigger( gg_trg_Paladins_leaderboard_update )
        call ForForce( GetPlayersAll(), function Trig_Paladins_leaderboard_update_Func009Func003002 )
        call DisplayTimedTextToForce( GetPlayersAll(), 30, ( GetPlayerName(GetOwningPlayer(GetKillingUnitBJ())) + " has first slain 5 Paladins and will be given a reward by Sylvanas Windrunner in the Undercity !" ) )
        call UnitRemoveItemFromSlotSwapped( 6, GetKillingUnitBJ() )
        call UnitAddItemByIdSwapped( 'I01W', GetKillingUnitBJ() )
        call EnableTrigger( gg_trg_ForsakenQuest_3b )
        call DisableTrigger( gg_trg_Paladins_on )
        call EnableTrigger( gg_trg_Paladins_off )
        call PlaySoundBJ( gg_snd_GameFound )
        call WaitForSoundBJ( gg_snd_GameFound, 0 )
        call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\SadMystery.mp3" )
        call PingMinimapLocForForce( GetPlayersAll(), GetRectCenter(gg_rct_Quest_Sylvanas), 5.00 )
    else
        call DoNothing(  )
    endif
endfunction

