    if ( not ( udg_undeadkills[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))] == 5 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Undead_leaderboard_update_Actions takes nothing returns nothing
    set udg_undeadkills[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))] = ( udg_undeadkills[GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ()))] + 1 )
    call ForForce( GetPlayersAll(), function Trig_Undead_leaderboard_update_Func008002 )
    call LeaderboardSortItemsBJ( udg_Crusade, bj_SORTTYPE_SORTBYVALUE, false )
    if ( Trig_Undead_leaderboard_update_Func010C() ) then
        call DisableTrigger( gg_trg_Undead_leaderboard_update )
        call ForForce( GetPlayersAll(), function Trig_Undead_leaderboard_update_Func010Func003002 )
        call DisplayTimedTextToForce( GetPlayersAll(), 30, ( GetPlayerName(GetOwningPlayer(GetKillingUnitBJ())) + " has first destroyed 5 undead Heroes and will be given a reward by King Anduin Wrynn in Azeroth !" ) )
        call UnitRemoveItemFromSlotSwapped( 6, GetKillingUnitBJ() )
        call UnitAddItemByIdSwapped( 'I01D', GetKillingUnitBJ() )
        call EnableTrigger( gg_trg_HumanQuest_4c )
        call DisableTrigger( gg_trg_Crusade_off )
        call DisableTrigger( gg_trg_Crusade_on )
        call PlaySoundBJ( gg_snd_UtherReturns )
        call WaitForSoundBJ( gg_snd_UtherReturns, 0 )
        call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\HumanVictory.mp3" )
        call PingMinimapLocForForce( GetPlayersAll(), GetRectCenter(gg_rct_Quest_Anduin), 5.00 )
    else
        call DoNothing(  )
    endif
endfunction

