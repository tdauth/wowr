function BuildExpansions takes nothing returns nothing
    call SetBuildExpa(expansions, FACELESS_ONE_TIER_3)
    call SetBuildExpa(expansions, FACELESS_ONE_PRISON)
    call SetBuildExpa(expansions, FACELESS_ONE_TENTACLE_PIT)
endfunction

function BuildShips takes nothing returns nothing
    if (buildShips) then
        call SetBuildUnit( 1, ORC_SHIPYARD )
        call SetBuildUnit( 1, ORC_TRANSPORT_SHIP )
        call SetBuildUnit( 2, ORC_FRIGATE )
        call SetBuildUnit( 2, ORC_JUGGERNAUGHT )
    else
        call SetBuildUnit( 0, ORC_SHIPYARD )
        call SetBuildUnit( 0, ORC_TRANSPORT_SHIP )
        call SetBuildUnit( 0, ORC_FRIGATE )
        call SetBuildUnit( 0, ORC_JUGGERNAUGHT )
    endif
endfunction

function AttackWaves takes nothing returns nothing
    //*** WAVE 1 ***
    call InitAssaultGroup()
    call CampaignAttackerEx( 2, 3, 3, FACELESS_ONE_UNBROCKEN_DARKHUNTER )
    call CampaignAttackerEx( 0, 1, 2, FACELESS_ONE_BONE_THROWER )
    call AddStandardUnitsToAttackWave()
    call Sleep( M3 ) // Waits 3 minutes before attacking
    call LaunchAttack()

    //*** WAVE 1 ***
    call InitAssaultGroup()
    call CampaignAttackerEx( 2, 3, 3, FACELESS_ONE_UNBROCKEN_DARKHUNTER )
    call CampaignAttackerEx( 0, 1, 2, FACELESS_ONE_BONE_THROWER )
    call AddStandardUnitsToAttackWave()
    call Sleep( M2 ) // Waits 2 minutes before attacking
    call LaunchAttack()

    //*** WAVE 2 ***
    call InitAssaultGroup()
    call CampaignAttackerEx( 3, 4, 4, FACELESS_ONE_UNBROCKEN_DARKHUNTER )
    call CampaignAttackerEx( 1, 2, 3, FACELESS_ONE_BONE_THROWER )
    call AddStandardUnitsToAttackWave()
    call Sleep( M1 ) // Waits 1 minute before attacking
    call LaunchAttack()

    //*** WAVE 3 ***
    call InitAssaultGroup()
    call CampaignAttackerEx( 3, 4, 4, FACELESS_ONE_UNBROCKEN_DARKHUNTER )
    call CampaignAttackerEx( 1, 2, 3, FACELESS_ONE_BONE_THROWER )
    call CampaignAttackerEx( 1, 2, 3, FACELESS_ONE_RIDER )
    call AddStandardUnitsToAttackWave()
    call Sleep( M2 ) // Waits 2 minutes before attacking
    call LaunchAttack()

    //*** WAVE 4 ***
    call InitAssaultGroup()
    call CampaignAttackerEx( 3, 4, 4, FACELESS_ONE_UNBROCKEN_DARKHUNTER )
    call CampaignAttackerEx( 1, 2, 3, FACELESS_ONE_BONE_THROWER )
    call CampaignAttackerEx( 1, 2, 3, FACELESS_ONE_RIDER )
    call CampaignAttackerEx( 1, 2, 3, FACELESS_ONE_BALLISTA )
    call AddStandardUnitsToAttackWave()
    call Sleep( M2 ) // Waits 2 minutes before attacking
    call LaunchAttack()

    //*** WAVE 5 ***
