    call SetBuildUpgr( 3, UPG_MURLOC_DAMAGE )
    call SetBuildUpgr( 3, UPG_MURLOC_ARMOR )
    call SetBuildUpgr( 2, UPG_MURLOC_BUBBLE_MAGE )
    call SetBuildUpgr( 2, UPG_MURLOC_SHADOW_CASTER )
    call SetBuildUpgr( 2, UPG_MURLOC_CULTIST )
    call SetBuildUpgr( 1, UPG_MURLOC_RIVER_BANK )
    call SetBuildUpgr( 1, UPG_MURLOC_EGGS )
    call SetBuildUpgr( 1, UPG_MURLOC_SWARMING )
    //call SetBuildUpgr( 1, UPG_OGRE_SUMMON_BEAR )
    //call SetBuildUpgr( 1, UPG_OGRE_WAR_STOMP )
    call SetBuildUpgr( 1, UPG_MURLOC_BACKPACK )
    //call SetBuildUpgr( 1, UPG_OGRE_GRONN_CLUBS )
    call SetBuildUpgr( 1, UPG_MURLOC_FISHER )
    call SetBuildUnit( 1, MURLOC_FISHER )

    call BuildingStrategyStandardTier3()
    // **********************************
    // *    End Building Strategy       *
    // **********************************
endfunction

function BuildExpansions takes nothing returns nothing
    call Sleep(M10)
    loop
        if (expansions > 0) then
            call SetBuildExpa(expansions, MURLOC_TIER_3)
            call SetBuildExpa(expansions, MURLOC_BLADEMAKING_HUT)
            call SetBuildExpa(expansions, MURLOC_LUMBER_KEEP )
            call SetBuildExpa(expansions, MURLOC_BONE_CHIPPER)
            call SetBuildExpa(expansions, MURLOC_BONE_CHIPPER)
        endif

        call Sleep(M1)
    endloop
endfunction

function BuildShips takes nothing returns nothing
    call Sleep(M10)
    loop
        if (buildShips) then
            call SetBuildUnit( 1, OGRE_SHIPYARD )
            call SetBuildUnit( 1, ORC_TRANSPORT_SHIP )
            call SetBuildUnit( 2, ORC_FRIGATE )
            call SetBuildUnit( 2, ORC_JUGGERNAUGHT )
        else
            call SetBuildUnit( 0, OGRE_SHIPYARD )
            call SetBuildUnit( 0, ORC_TRANSPORT_SHIP )
            call SetBuildUnit( 0, ORC_FRIGATE )
            call SetBuildUnit( 0, ORC_JUGGERNAUGHT )
        endif

        call Sleep(M1)
    endloop
endfunction

function AttackWaves takes nothing returns nothing
